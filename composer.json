{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.8",
        "doctrine/doctrine-migrations-bundle": "*",
        "doctrine/orm": "^2.14",
        "dunglas/doctrine-json-odm": "^1.4",
        "endroid/qr-code-bundle": "^6.0",
        "nelmio/cors-bundle": "^2.2",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.15",
        "scienta/doctrine-json-functions": "^5.2",
        "stripe/stripe-php": "^12.3",
        "symfony/asset": "7.*.*",
        "symfony/config": "7.*.*",
        "symfony/console": "7.*.*",
        "symfony/dependency-injection": "7.*.*",
        "symfony/doctrine-messenger": "7.*.*",
        "symfony/dotenv": "7.*.*",
        "symfony/event-dispatcher": "7.*.*",
        "symfony/expression-language": "7.*.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "*",
        "symfony/http-client": "7.*.*",
        "symfony/http-foundation": "7.*.*",
        "symfony/intl": "7.*.*",
        "symfony/lock": "7.*.*",
        "symfony/mailer": "7.*.*",
        "symfony/messenger": "7.*.*",
        "symfony/mime": "7.*.*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "7.*.*",
        "symfony/process": "7.*.*",
        "symfony/property-access": "*",
        "symfony/property-info": "*",
        "symfony/runtime": "7.*.*",
        "symfony/security-bundle": "7.*.*",
        "symfony/serializer": "7.*.*",
        "symfony/string": "7.*.*",
        "symfony/translation": "7.*.*",
        "symfony/twig-bundle": "^6.0",
        "symfony/validator": "6.*.*",
        "symfony/web-link": "7.*.*",
        "symfony/workflow": "7.*.*",
        "symfony/yaml": "7.*.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0",
        "twilio/sdk": "^7.5"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true,
            "endroid/installer": false
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "RI5\\" : "lib/RI5Bundle/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.*.*"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "7.*.*",
        "symfony/css-selector": "7.*.*",
        "symfony/debug-bundle": "7.*.*",
        "symfony/maker-bundle": "^1.48",
        "symfony/phpunit-bridge": "^6.3",
        "symfony/stopwatch": "7.*.*",
        "symfony/web-profiler-bundle": "7.*.*"
    }
}
